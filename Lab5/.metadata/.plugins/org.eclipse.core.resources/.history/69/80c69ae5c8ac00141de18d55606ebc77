import java.util.Arrays;


public class NbyN {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int m=StdIn.readInt();
		int n=StdIn.readInt();
		int[][] a=new int[m][n];
		for(int i=0;i<m;i++)
			for(int j=0;j<n;j++)
				if(i==j) a[i][j]=i; else a[i][j]=0;
		System.out.println(Arrays.toString(a));
		

	}

}
