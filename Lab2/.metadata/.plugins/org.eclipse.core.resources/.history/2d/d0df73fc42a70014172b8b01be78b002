
public class ArrayRotate {
	public static void main(String[] args) {
		int n=StdIn.readInt();
		int[] nums;
		nums=new int[n];
		for(int i=0;i<n;i++)
			nums[i]=StdIn.readInt();
		int[] copy=new int[n];
		for(int i=0;i<n;i++)
			copy[i]=nums[(i+1)%n];
		for(int i=0;i<n;i++)
			StdOut.print(copy[i]+" ");
		StdOut.println();
		StdOut.print("direct rotate: "+'\n');
		int temp;
		for(int i=0;i<n-1;i++)
				{ temp=num[i];
				  num[i]=num[i+1];
				  num[i+1]=num[i];
				}
		for(int i=0;i<n;i++)
			StdOut.print(num[i]+" ");
			
			
		StdOut.close();
		
	}

}
