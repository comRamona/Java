
public class Sieve {
public static void main(String[] args){ 
	int[] all;
	all=new int[20];
	for(int i=1;i<=19;i++)
		all[i]=i;
	int p=2,i=0;
	while(p<19) {
		System.out.print(p+" ");
		for(i=2*p;i<19;i+=p)
			all[i]=0;
		for(i=1;i<19;i++)
		System.out.print(all[i]+" ");
		System.out.println();
		p++; 
	}
}
}
