
public class Sieve {
public static void main(String[] args){ 
	int[] all;
    int n=StdIn.readInt();
	all=new int[n-1];
	for(int i=0;i<n-1;i++)
		all[i]=i+2;
	 int p=2,i=0;
	while(p<n) {
		System.out.print(p+" ");
		for(i=2*p;i<n-1;i+=p)
			{all[i]=0; System.out.print(all[i]+" "); }
		System.out.println();
		boolean w=true;
		for(i=p-2;i<n-1&&w;i++)
			if(all[i]>p) {w=false; p=all[i];}
		if(w) p=n; 
			
	} 
}
}
