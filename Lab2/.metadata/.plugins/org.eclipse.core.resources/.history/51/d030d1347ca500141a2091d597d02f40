import java.io.*;
public class arr {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		BufferedReader r=new BufferedReader(new InputStreamReader(System.in));
		int m=Integer.parseInt(r.readLine());
		int n=Integer.parseInt(r.readLine());
		
       for(int i=0;i<=m;i++)
    	   {for(int j=0;j<=n;j++)
    		   System.out.println("*");
    	   System.out.println();}
	}

}
