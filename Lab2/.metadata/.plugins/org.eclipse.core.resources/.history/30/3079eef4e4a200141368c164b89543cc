
class number {
	public static real(int x) { return x; }
	public static imag(int y) {return y; }
}
public class Imagiary {
  public static void main() {
	  int A=StdIn.readInt();
	  int B=StdIn.readInt();
	  int C=StdIn.readInt();
	  double delta=B*B-4*A*C;
	  if(delta>=0) { double x1=(-B+Math.sqrt(delta))/(2*A); double x2=(-B-Math.sqrt(delta))/(2*A);}
	  else { number x1=new number(); x1.real((-B)/2*A); x1.imag=Math.sqrt(Math.abs(delta))
		  
  }
}
