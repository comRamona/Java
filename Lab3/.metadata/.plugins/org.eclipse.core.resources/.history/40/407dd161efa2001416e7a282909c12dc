
public class Sieve {
public static void main(String[] args){ 
	int[] all;
	all=new int[21];
	for(int i=1;i<=20;i++)
		all[i]=i;
	int p=2,i=0;
	while(p<19) {
		System.out.print(p+" ");
		for(i=2*p;i<=20;i+=p)
			all[i]=0;
		for(i=1;i<=20;i++)
		System.out.print(all[i]+" ");
		System.out.println();
		boolean w=true;
		for(i=p+1;i<=20&&w;i++)
			if(all[i]>p) {w=false; p=all[i];}
		if(w) p=20;
			
	}
}
}
